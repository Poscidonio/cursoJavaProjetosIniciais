package entities;

import java.lang.FdLibm.Pow;

public class Rectangle {
	
	// atributos do retangulo
	public double width;
	public double height;
	
	
	public double area() {
		return width * height;
	}
	
	public double perimeter() {
		return 2 * (width + height);
	}
	
	public double diagonal() {
		return Math.sqrt(Math.pow((width + height),2));
	}

}
